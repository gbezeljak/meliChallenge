    "@babel/preset-react": "^7.12.10",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.24.0",
    "history": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.2",
